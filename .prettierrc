{
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSameLine": false,
  "objectWrap": "preserve",
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "printWidth": 80,
  "endOfLine": "auto",
  "arrowParens": "always",
  "overrides": [
    {
      "files": "*.yml",
      "options": {
        "tabWidth": 8
      }
    }
  ]
}
